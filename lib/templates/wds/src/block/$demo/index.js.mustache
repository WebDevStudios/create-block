	keywords: [
		__( '{{ namespace }}', '{{ textdomain }}' ),
		__( '{{ slug }}', '{{ textdomain }}' ),
	],
	attributes: {
		content: {
			type: 'array',
			source: 'children',
			selector: 'p',
		},
	},
	edit,
	save,
} );
